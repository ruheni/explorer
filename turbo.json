{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "SOURCIFY_URL",
        "AWS_S3_ACCESSKEY_ID",
        "AWS_S3_ACCESSKEY_SECRET"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#build": {
      "env": [
        "WHITELABEL",
        "CELESTIA_MOCHA_RPC",
        "DYMENSION_HUB_RPC",
        "DYMENSION_ROLLAPP_X_RPC",
        "ADD_NETWORK_PASS",
        "ADD_NETWORK_ENDPOINT",
        "EVM_CHAIN_DATA_SERVICE",
        "SOLANA_RPC",
        "VERCEL_URL",
        "METRICS_API_URL",
        "ETHEREUM_RPC",
        "NEXT_PUBLIC_VERCEL_URL",
        "SOURCIFY_URL",
        "AWS_S3_ACCESSKEY_SECRET",
        "AWS_S3_ACCESSKEY_ID",
        "NODE_ENV",
        "UPLOADCARE_API_KEY",
        "IPFS_GATEWAY",
        "CELESTIA_BLOCKSPACE_RACE_RPC",
        "NAMESPACE_ENDPOINT",
        "CELESTIA_ARABICA_RPC"
      ]
    },
    "verify#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "SOURCIFY_URL",
        "AWS_S3_ACCESSKEY_ID",
        "AWS_S3_ACCESSKEY_SECRET"
      ]
    }
  }
}
